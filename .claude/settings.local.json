      "Bash(./mvnw.cmd spring-boot:run:*)",
      "Bash(timeout /t 15 /nobreak)",
      "Bash(powershell -Command \"Start-Sleep -Seconds 10\")",
      "Bash(powershell:*)",
      "Bash(taskkill:*)",
      "Bash(netstat:*)",
      "Bash(findstr:*)"
    ],
    "deny": [],
    "ask": []
  }
}
